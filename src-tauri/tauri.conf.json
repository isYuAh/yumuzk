{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist"
  },
  "package": {
    "productName": "yumuzk",
    "version": "0.0.0"
  },
  "tauri": {
    "allowlist": {
      "all": true,
      "shell": {
        "all": false,
        "open": true,
        "scope": [{
          "name": "node",
          "cmd": "node",
          "args": ["./res/neteaseapi/app.js"]
        }]
      },
      "fs": {
        "scope": ["$RESOURCE/**"]
      },
      "http": {
        "scope": ["http://**", "https://**"]
      },
      "protocol": {
        "assetScope": ["**"]
      }
    },
    "windows": [
      {
        "label": "main",
        "title": "yumuzk",
        "width": 1080,
        "height": 720,
        "titleBarStyle": "Transparent",
        "decorations": false,
        "resizable": false
      }
    ],
    "security": {
      "csp": null
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "top.isyuah.dev",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    }
  }
}
